{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "files": {
    "include": ["**/*.css"]
  },
  "css": {
    "linter": {
      "enabled": true
    },
    "parser": {
      "cssModules": false
    }
  },
  "linter": {
    "enabled": true,
    "rules": {
      "nursery": {
        "noDuplicateCustomProperties": "error"
      },
      "suspicious": {
        "noEmptyBlock": "error"
      }
    }
  }
}